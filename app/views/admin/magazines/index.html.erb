<p id="notice"><%= notice %></p>

<h1>Listing Magazines</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @magazines.each do |magazine| %>
      <tr>
        <td><%= magazine.name %></td>
        <td><%= link_to 'Show', admin_magazine_path(magazine) %></td>
        <td><%= link_to 'Edit', edit_admin_magazine_path(magazine) %></td>
        <td><%= link_to 'Destroy', admin_magazine_path(magazine), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Magazine', new_admin_magazine_path %>
